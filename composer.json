{
    "name": "fabacino/php-debug-functions",
    "description": "Debug functions for PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabian Wiget",
            "email": "fabacino@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "monolog/monolog": "^1.23"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Fabacino\\Debug\\": "src"
        },
        "files": [
            "src/debug-functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Fabacino\\Debug\\Test\\": "tests"
        }
    }
}
